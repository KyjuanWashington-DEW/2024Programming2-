<Page x:Class="DEWsOggleWPF.EndingPages"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DEWsOggleWPF"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="EndingPages">

    <Grid Background="#FF2B2B2B">
        <TextBlock x:Name="LeaderboardTextBlock" HorizontalAlignment="Center" Margin="0,98,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Foreground="White" FontSize="20"/>
        <Button x:Name="YesButton" Content="Yes" HorizontalAlignment="Left" Margin="293,339,0,0" VerticalAlignment="Top" Height="50" Width="107" Click="Button_Click"/>
        <Button x:Name="NoButton" Content="No" HorizontalAlignment="Left" Margin="400,339,0,0" VerticalAlignment="Top" Height="50" Width="107" Click="NoButton_Click"/>
        <Button x:Name="ByeButton" Content="Button" HorizontalAlignment="Center" Margin="0,205,0,0" VerticalAlignment="Top" Click="ByeButton_Click" Visibility="Hidden"/>

    </Grid>
</Page>
